<!DOCTYPE html>
<html lang="en">
  <!-- Head-->
  <head>
    <!-- Title -->
    <title>Logoipsum</title>

    <!-- SEO -->
    <meta name="description" content="" />

    <!-- Favicons -->

    <!-- Launching CSS -->
    <link rel="stylesheet" href="css/style.css" />

    <!-- Charset -->
    <meta charset="UTF-8" />

    <!-- Adaptive meta tegs -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  </head>
  <!-- Body-->
  <body>
    <div class="wrapper">
      <!-- Header start -->
      <header class="header">
        <div class="header__container _container">
          <div class="header__body">
            <a href="/" class="header__logo">
              <img
                src="img/logos/header/company-logo.svg"
                alt="Company-logo"
                class="header__logo-pic"
              />
            </a>

            <nav class="header__menu menu">
              <ul class="menu__list">
                <li class="menu__item">
                  <a href="#!" class="menu__link">Home</a>
                </li>

                <li class="menu__item">
                  <a href="#!" class="menu__link">Features</a>
                </li>

                <li class="menu__item">
                  <a href="#!" class="menu__link">Pricing</a>
                </li>

                <li class="menu__item">
                  <a href="#!" class="menu__link">Blog</a>
                </li>
              </ul>
            </nav>

            <a href="#!" class="header__button primaryBtn">Get Started</a>
          </div>
        </div>
      </header>
      <!-- Header end -->

      <!-- Page start -->
      <main class="page">
        <div class="home">
          <div class="home__container">
            <div class="home__body">
              <div class="home__content">
                <h1 class="home__title">Grow your subscription business</h1>
                <p class="home__text">
                  Outcome-centered products that reduce churn, optimize pricing,
                  and grow your subscription business end-to-end.
                </p>
                <div class="home__button button">
                  <a href="#!" class="button__link homeBtn">Get Started</a>
                  <a href="#!" class="button__icon">
                    <img
                      src="/img/icons/home/arrow-ico.svg"
                      alt="Get Started"
                      class="button__icon-arrow"
                    />
                  </a>
                </div>
              </div>

              <div class="home__img">
                <img
                  src="img/content/home/home-pic.png"
                  alt="Grow your
                subscription
                business"
                  class="home__img-pic"
                />
              </div>
            </div>
          </div>
        </div>
      </main>
      <!-- Page end -->

      <!-- Footer start -->
      <footer class="footer"></footer>
      <!-- Footer end -->
    </div>

    <!-- Scripts -->
    <script src="js/main.js"></script>
    <!-- Scripts -->
  </body>
</html>
