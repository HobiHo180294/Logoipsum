<!DOCTYPE html>
<html lang="en">
  <!-- Head-->
  <head>
    <!-- Title -->
    <title>Logoipsum</title>

    <!-- SEO -->
    <meta name="description" content="" />

    <!-- Favicons -->
    <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="img/favicon/apple-touch-icon.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="img/favicon/favicon-32x32.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="img/favicon/favicon-16x16.png"
    />
    <link rel="manifest" href="img/favicon/site.webmanifest" />
    <link
      rel="mask-icon"
      href="img/favicon/safari-pinned-tab.svg"
      color="#5bbad5"
    />
    <link rel="shortcut icon" href="img/favicon/favicon.ico" />
    <meta name="msapplication-TileColor" content="#da532c" />
    <meta name="msapplication-config" content="img/favicon/browserconfig.xml" />
    <meta name="theme-color" content="#ffffff" />

    <!-- Launching CSS -->
    <link rel="stylesheet" href="css/style.css" />

    <!-- Charset -->
    <meta charset="UTF-8" />

    <!-- Adaptive meta tegs -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  </head>
  <!-- Body-->
  <body>
    <div class="wrapper">
      <!-- Header start -->
      <header class="header">
        <div class="header__container _container">
          <div class="header__body">
            <a href="/" class="header__logo">
              <img
                src="img/logos/header/company-logo.svg"
                alt="Company-logo"
                class="header__logo-pic"
              />
            </a>

            <div class="header__burger">
              <span></span>
            </div>

            <nav class="header__menu menu">
              <ul class="menu__list">
                <li class="menu__item">
                  <a href="#home" class="menu__link js-scroll">Home</a>
                </li>

                <li class="menu__item">
                  <a href="#features" class="menu__link js-scroll">Features</a>
                </li>

                <li class="menu__item">
                  <a href="#pricing" class="menu__link js-scroll">Pricing</a>
                </li>

                <li class="menu__item">
                  <a href="#blog" class="menu__link js-scroll">Blog</a>
                </li>

                <li class="menu__item">
                  <a href="#contact" class="menu__link js-scroll">Contact</a>
                </li>
              </ul>
            </nav>

            <a href="#contact" class="header__button primaryBtn js-scroll">
              Get Started
            </a>
          </div>
        </div>
      </header>
      <!-- Header end -->

      <!-- Page start -->
      <main class="page">
        <!-- Home block start -->
        <div class="page__home home" id="home">
          <div class="home__container">
            <div class="home__body">
              <div class="home__content">
                <h1 class="home__title">Grow your subscription business</h1>
                <p class="home__text">
                  Outcome-centered products that reduce churn, optimize pricing,
                  and grow your subscription business end-to-end.
                </p>
                <div class="home__button button">
                  <a href="#blog" class="button__link homeBtn js-scroll">
                    Get Started
                  </a>
                  <a href="#blog" class="button__icon js-scroll">
                    <img
                      src="img/icons/home/arrow-ico.svg"
                      alt="Get Started"
                      class="button__icon-arrow"
                    />
                  </a>
                </div>
              </div>

              <div class="home__img _ibg">
                <img
                  src="img/content/home/home-pic.png"
                  alt="Grow your
                subscription
                business"
                  class="home__img-pic"
                />
              </div>
            </div>
          </div>
        </div>
        <!-- Home block end -->

        <!-- Partners block start -->
        <div class="page__partners partners">
          <div class="partners__container _container">
            <div class="partners__items">
              <a href="#!" class="partners__item">
                <img
                  src="img/logos/partners/ball-logo.svg"
                  alt="ball-logo"
                  class="partners__item-logo"
                />
              </a>

              <a href="#!" class="partners__item">
                <img
                  src="img/logos/partners/triangles-logo.svg"
                  alt="triangles-logo"
                  class="partners__item-logo"
                />
              </a>

              <a href="#!" class="partners__item">
                <img
                  src="img/logos/partners/shapes-logo.svg"
                  alt="shapes-logo"
                  class="partners__item-logo"
                />
              </a>

              <a href="#!" class="partners__item">
                <img
                  src="img/logos/partners/beach-logo.svg"
                  alt="beach-logo"
                  class="partners__item-logo"
                />
              </a>

              <a href="#!" class="partners__item">
                <img
                  src="img/logos/partners/square-logo.svg"
                  alt="square-logo"
                  class="partners__item-logo"
                />
              </a>
            </div>
          </div>
        </div>
        <!-- Partners block end -->

        <!-- Blog section start -->
        <section class="page__blog blog" id="blog">
          <div class="blog__container _container">
            <div class="blog__body">
              <div class="blog__img _ibg">
                <img
                  src="img/content/blog/diagram.png"
                  alt="Stats"
                  class="blog__img-pic"
                />
              </div>

              <div class="blog__description description">
                <div class="description__body">
                  <h2 class="description__title">Subscription index</h2>
                  <p class="description__text">
                    A daily dataset to keep you up to date on
                    <br />
                    subscription market trends and what's happening in
                    <br />
                    your vertical.
                  </p>
                  <a href="#features" class="description__more js-scroll">
                    Learn more
                  </a>
                </div>
              </div>

              <div class="blog__img data__img _ibg">
                <img
                  src="img/content/blog/graph.png"
                  alt="Data"
                  class="blog__img-pic"
                />
              </div>

              <div class="blog__description description">
                <div class="description__body">
                  <h2 class="description__title">In-depth metrics</h2>
                  <p class="description__text">
                    Accurate, real-time reporting at your fingertips.
                    <br />
                    Getting data has never been easier.
                  </p>
                  <a href="#features" class="description__more js-scroll">
                    Learn more
                  </a>
                </div>
              </div>
            </div>
          </div>
        </section>
        <!-- Blog section end -->

        <!-- Features section start -->
        <section class="page__features features" id="features">
          <div class="features__container _container">
            <div class="features__body">
              <div class="features__column">
                <div class="features__item item-features">
                  <div class="item-features__icon _ibg">
                    <img
                      src="img/content/features/card.png"
                      alt="Benchmarks"
                      class="item-features__icon-pic"
                    />
                  </div>

                  <h3 class="item-features__title">Benchmarks</h3>
                  <p class="item-features__text">
                    See how you stack up against comparable companies in similar
                    stages.
                  </p>
                </div>
              </div>

              <div class="features__column">
                <div class="features__item item-features">
                  <div class="item-features__icon _ibg">
                    <img
                      src="img/content/features/calculator.png"
                      alt="Pricing Audit"
                      class="item-features__icon-pic"
                    />
                  </div>

                  <h3 class="item-features__title">Pricing Audit</h3>
                  <p class="item-features__text">
                    Benchmark the health of your monetization and pricing
                    strategy.
                  </p>
                </div>
              </div>

              <div class="features__column">
                <div class="features__item item-features">
                  <div class="item-features__icon _ibg">
                    <img
                      src="img/content/features/camera.png"
                      alt="Retention Audit"
                      class="item-features__icon-pic"
                    />
                  </div>

                  <h3 class="item-features__title">Retention Audit</h3>
                  <p class="item-features__text">
                    Audit where revenue leakage exists and where you can
                    increase retention.
                  </p>
                </div>
              </div>
            </div>
          </div>
        </section>
        <!-- Features section end -->

        <!-- Pricing section start -->
        <section class="page__pricing pricing" id="pricing">
          <div class="pricing__container">
            <div class="pricing__header">
              <h2 class="pricing__title">
                Get the right plan
                <br />
                for future product.
              </h2>
              <div class="pricing__slider slider">
                <a href="#!" class="slider__yearly">Yearly</a>
                <a href="#monthly" class="slider__monthly js-scroll">Monthly</a>
              </div>
            </div>

            <div class="pricing__body">
              <div class="pricing__column" id="monthly">
                <div class="pricing__item item-pricing">
                  <div class="item-pricing__class">Starter</div>
                  <div class="item-pricing__cost"><mark>Free</mark></div>
                  <div class="item-pricing__info info">
                    <p class="info__website service">1 Website</p>
                    <p class="info__memory service">5 GB Hosting</p>
                    <p class="info__support service">Limited Support</p>
                  </div>

                  <a
                    href="#contact"
                    class="item-pricing__button cardBtn js-scroll"
                  >
                    Get Started
                  </a>
                </div>
              </div>

              <div class="pricing__column">
                <div class="pricing__item item-pricing">
                  <div class="item-pricing__class">Premium</div>
                  <div class="item-pricing__cost">
                    <mark>$29</mark>
                    /month
                  </div>
                  <div class="item-pricing__info info">
                    <p class="info__website service">10 Website</p>
                    <p class="info__memory service">15 GB Hosting</p>
                    <p class="info__support service">Premium Support</p>
                  </div>

                  <a
                    href="#contact"
                    class="item-pricing__button cardBtn js-scroll"
                  >
                    Get Started
                  </a>
                </div>
              </div>

              <div class="pricing__column">
                <div class="pricing__item item-pricing">
                  <div class="item-pricing__class">Enterprise</div>
                  <div class="item-pricing__cost">
                    <mark>$49</mark>
                    /month
                  </div>
                  <div class="item-pricing__info info">
                    <p class="info__website service">Unlimited Website</p>
                    <p class="info__memory service">50 GB Hosting</p>
                    <p class="info__support service">Premium Support</p>
                  </div>

                  <a
                    href="#contact"
                    class="item-pricing__button cardBtn js-scroll"
                  >
                    Get Started
                  </a>
                </div>
              </div>
            </div>
          </div>
        </section>
        <!-- Pricing section end -->

        <!-- Electronic queue section start -->
        <section class="page__queue queue" id="contact">
          <div class="queue__container _container">
            <div class="queue__body">
              <div class="queue__content">
                <h2 class="queue__title">
                  Develop your business
                  <br />
                  with us
                </h2>
                <div class="queue__text">
                  Fill out the form and sign up with our specialists.
                  <br />
                  We'll find the best for you!
                </div>
              </div>

              <form action="#" class="queue__form form-queue">
                <fieldset class="form-queue__group">
                  <div class="form-queue__group-input name-group">
                    <label
                      for="nameInput"
                      class="name-group__label field-label"
                    >
                      Your name
                    </label>
                    <input
                      type="text"
                      name="customerName"
                      id="nameInput"
                      class="name-group__field input-field"
                    />
                    <!-- <div class="errors"></div> -->
                  </div>
                  <div class="form-queue__group-input phone-group">
                    <label
                      for="phoneInput"
                      class="phone-group__label field-label"
                    >
                      Your phone
                    </label>
                    <input
                      type="tel"
                      name="customerPhone"
                      data-tel-input=""
                      maxlength="18"
                      id="phoneInput"
                      class="phone__field input-field"
                    />
                    <!-- <div class="errors"></div> -->
                  </div>

                  <div class="form-queue__group-input country-group">
                    <h3 class="country-group__title">Your country</h3>

                    <div class="country-group__select-box">
                      <div class="country-group__options-container">
                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="USA"
                            name="country"
                          />
                          <label class="country-group__label" for="USA">
                            USA
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="China"
                            name="country"
                          />
                          <label class="country-group__label" for="China">
                            China
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="Japan"
                            name="country"
                          />
                          <label class="country-group__label" for="Japan">
                            Japan
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="Germany"
                            name="country"
                          />
                          <label class="country-group__label" for="Germany">
                            Germany
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="India"
                            name="country"
                          />
                          <label class="country-group__label" for="India">
                            India
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="UK"
                            name="country"
                          />
                          <label class="country-group__label" for="UK">
                            Great Britain
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="France"
                            name="country"
                          />
                          <label class="country-group__label" for="France">
                            France
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="Italy"
                            name="country"
                          />
                          <label class="country-group__label" for="Italy">
                            Italy
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="Brazil"
                            name="country"
                          />
                          <label class="country-group__label" for="Brazil">
                            Brazil
                          </label>
                        </div>

                        <div class="country-group__option">
                          <input
                            type="radio"
                            class="country-group__radio"
                            id="Ukraine"
                            name="country"
                          />
                          <label class="country-group__label" for="Ukraine">
                            Ukraine
                          </label>
                        </div>
                      </div>

                      <div class="country-group__selected">
                        Select your country
                      </div>
                      <!-- <div class="errors"></div> -->
                    </div>
                  </div>

                  <div class="form-queue__group-input business-realm-group">
                    <label
                      for="realmInput"
                      class="business-realm-group__label field-label"
                    >
                      Scope of your business
                    </label>
                    <input
                      type="text"
                      name="businessRealm"
                      id="realmInput"
                      class="business-realm-group__field input-field"
                    />
                    <!-- <div class="errors"></div> -->
                  </div>

                  <div class="form-queue__group-input time-group">
                    <label
                      for="timeInput"
                      class="time-group__label field-label"
                    >
                      Favorable time to contact
                    </label>
                    <input
                      type="datetime-local"
                      name="callTime"
                      id="timeInput"
                      class="time-group__field input-field"
                    />
                    <!-- <div class="errors"></div> -->
                  </div>

                  <button
                    class="form-queue__group-button homeBtn"
                    type="submit"
                  >
                    Start your growth
                  </button>
                </fieldset>
              </form>
            </div>
          </div>
        </section>
        <!-- Electronic queue section end -->
      </main>
      <!-- Page end -->

      <!-- Footer start -->
      <footer class="footer">
        <div class="footer__wrap _container">
          <div class="footer__body">
            <div class="footer__column">
              <h3 class="footer__label">Company Info</h3>
              <nav class="footer__menu menu-footer">
                <ul class="menu-footer__list">
                  <li class="menu-footer__item">
                    <a href="#home" class="menu-footer__link js-scroll">Home</a>
                  </li>

                  <li class="menu-footer__item">
                    <a href="#features" class="menu-footer__link js-scroll">
                      Features
                    </a>
                  </li>

                  <li class="menu-footer__item">
                    <a href="#pricing" class="menu-footer__link js-scroll">
                      Pricing
                    </a>
                  </li>

                  <li class="menu-footer__item">
                    <a href="#blog" class="menu-footer__link js-scroll">Blog</a>
                  </li>

                  <li class="menu-footer__item">
                    <a href="#contact" class="menu-footer__link js-scroll">
                      Electronic queue
                    </a>
                  </li>
                </ul>
              </nav>
            </div>

            <div class="footer__column">
              <h3 class="footer__label">Features</h3>
              <nav class="footer__menu menu-footer">
                <ul class="menu-footer__list">
                  <li class="menu-footer__item">Benchmarks</li>

                  <li class="menu-footer__item">Pricing Audit</li>

                  <li class="menu-footer__item">Retention Audit</li>

                  <li class="menu-footer__item">Unlimited Support</li>
                </ul>
              </nav>
            </div>

            <div class="footer__column">
              <h3 class="footer__label">Get In Touch</h3>
              <div class="footer__contacts contacts-footer">
                <a
                  href="tel:+380999999999"
                  class="contacts-footer__item contacts-footer__item_phone"
                >
                  +38 (099) 999-99-99
                </a>

                <address
                  class="contacts-footer__item contacts-footer__item_address"
                >
                  4517 Washington Ave. Manchester, Kentucky 39495
                </address>

                <a
                  href="mailto:logoipsum@gmail.com"
                  class="contacts-footer__item contacts-footer__item_email"
                >
                  logoipsum@gmail.com
                </a>
              </div>
            </div>
          </div>
        </div>
      </footer>
      <!-- Footer end -->
    </div>
    <!-- Scripts -->
    <script src="js/main.js"></script>
    <!-- Scripts -->
  </body>
</html>
